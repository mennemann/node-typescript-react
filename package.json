{
  "name": "node-typescipt-react",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build-webpack": "webpack",
    "build-scss": "node-sass .\\frontend\\public\\styles\\scss -o .\\frontend\\public\\styles\\css --output-style compressed",
    "build-ts": "rmdir build /s /q 2>nul && npx tsc",
    "watch-webpack": "webpack --watch",
    "watch-scss": "nodemon -e scss -x \"npm run-script build-scss\"",
    "watch-ts": "nodemon --config \"./nodemon.json\"/",
    "watch": "concurrently --kill-others \"npm run-script watch-webpack\" \"npm run-script watch-scss\" \"npm run-script watch-ts\"",
    "build": "concurrently --kill-others \"npm run-script build-webpack\" \"npm run-script build-scss\" \"npm run-script build-ts\"",
    "dev": "npm run-script watch",
    "start": "node build/start.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mennemann/node-typescipt-react.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "keywords": [],
